{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all",
  ],
  "enabled": true,
  "automergeStrategy": "merge-commit",
  "pin": {
    "automerge": true,
  },
  "digest": {
    "automerge": true,
  },
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "timezone": "America/New_York",
  "configMigration": true,
  "onboarding": false,
  "requireConfig": "ignored",
  "dependencyDashboard": true,
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "schedule": [
    "after 8am",
    "before 11pm"
  ],
  "automergeSchedule": [
    "after 8am",
    "before 11pm"
  ],
  "rebaseWhen": "conflicted",
  "assigneesFromCodeOwners": true,
  "suppressNotifications": [
    "prEditedNotification",
    "prIgnoreNotification"
  ],
  "docker-compose": {
    "enabled": true
  },
  "dockerfile": {
    "enabled": true
  },
  "pre-commit": {
    "enabled": true
  },
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "ignoreTests": true,
  "branchPrefix": "renovate/",
  "semanticCommits": "enabled",
  "ignorePaths": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**",
    "**/examples/**",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**",
    "**/__fixtures__/**",
    "**/*.sops.*",
    "**/.archive/**",
    "**/resources/**"
  ],
}
